{
  "name": "react-final-form-antd",
  "version": "0.0.1",
  "description": "antd react-final-form bindings",
  "main": "./lib/index.js",
  "jsnext:main": "./es/index.js",
  "homepage": "https://sophilabs-forks.github.io/react-final-form-antd",
  "repository": {
    "type": "git",
    "url": "https://github.com/sophilabs-forks/react-final-form-antd"
  },
  "scripts": {
    "build": "npm run build:lib && npm run build:es",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build:lib": "babel src --out-dir lib",
    "deploy": "gh-pages -d storybook-static",
    "predeploy": "npm run build-storybook",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "antd": "^2.13.8",
    "react": ">= 15",
    "react-final-form": "^1.1.1"
  },
  "devDependencies": {
    "@storybook/addon-knobs": "^3.0.0",
    "@storybook/react": "^3.0.0",
    "antd": "^2.13.8",
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.0.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-preset-env": "^1.5.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "cross-env": "^5.0.0",
    "eslint": "3.19.0",
    "eslint-loader": "1.7.1",
    "final-form": "^1.2.1",
    "gh-pages": "^1.0.0",
    "prop-types": "^15.6.0",
    "react": "^15.5.4",
    "react-dom": "*",
    "react-final-form": "^1.1.1",
    "yargs": "7.1.0"
  },
  "keywords": [
    "react-final-form",
    "antd",
    "bindings"
  ],
  "author": "Sophilabs",
  "license": "MIT",
  "bugs": {}
}
